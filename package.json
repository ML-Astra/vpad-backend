{
  "name": "TODO-backend",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./lib/src/main/server.prod.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && resolve-tspaths",
    "start": "yarn build && resolve-tspaths && pm2 start lib/src/main/server.prod.js",
    "stop": "pm2 stop server.prod",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only ./src/main/server.dev.ts --watch-files './docs/swagger.yaml'",
    "test": "jest --no-cache --coverage --silent --forceExit --runInBand --detectOpenHandles",
    "test-watch": "jest --no-cache --silent=false --runInBand --watch --no-coverage",
    "migrate": "prisma migrate && prisma generate",
    "lint": "eslint . --ext js,ts --fix",
    "check": "tsc && resolve-tspaths --noEmit"
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
    "@types/backblaze-b2": "^1.5.6",
    "@types/bcrypt": "^5.0.2",
    "@types/typescript": "^2.0.0",
    "aws-sdk": "^2.1323.0",
    "backblaze-b2": "^1.7.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pm2": "^5.3.0",
    "prisma": "^5.6.0",
    "resolve-tspaths": "^0.8.17",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.14.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "yarn-audit-fix": "^9.3.9"
  }
